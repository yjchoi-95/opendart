<TD ALIGN="RIGHT" VALIGN="MIDDLE" WIDTH="106" HEIGHT="30">95</TD>
<TD ALIGN="RIGHT" VALIGN="MIDDLE" WIDTH="106" HEIGHT="30">1,463</TD>
<TD ALIGN="RIGHT" VALIGN="MIDDLE" WIDTH="106" HEIGHT="30" USERMARK=" 0XFF0042 B">818</TD>
